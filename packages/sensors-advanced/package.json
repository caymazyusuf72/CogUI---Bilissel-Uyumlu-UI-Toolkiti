{
  "name": "@cogui/sensors-advanced",
  "version": "0.1.0",
  "description": "CogUI Advanced Sensor Systems - Eye tracking, voice analysis, biometric monitoring, and environmental sensors",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogui/cogui.git",
    "directory": "packages/sensors-advanced"
  },
  "keywords": [
    "cogui",
    "eye-tracking",
    "voice-analysis",
    "biometric-monitoring",
    "gesture-recognition",
    "environmental-sensors",
    "webrtc",
    "computer-vision",
    "accessibility",
    "cognitive-ui"
  ],
  "author": "CogUI Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-models": "^0.2.1",
    "webgazer": "^3.0.0",
    "face-api.js": "^0.22.2",
    "opencv-js": "^4.8.0",
    "ml-matrix": "^6.10.5",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "typescript": "^5.1.6",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0"
  },
  "peerDependencies": {
    "@cogui/core": "workspace:*",
    "@cogui/sensors": "workspace:*",
    "@cogui/ml": "workspace:*",
    "react": ">=16.8.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eye-tracking": {
      "import": "./dist/eye-tracking/index.js",
      "require": "./dist/eye-tracking/index.js",
      "types": "./dist/eye-tracking/index.d.ts"
    },
    "./voice-analysis": {
      "import": "./dist/voice-analysis/index.js",
      "require": "./dist/voice-analysis/index.js",
      "types": "./dist/voice-analysis/index.d.ts"
    },
    "./biometric": {
      "import": "./dist/biometric/index.js",
      "require": "./dist/biometric/index.js",
      "types": "./dist/biometric/index.d.ts"
    },
    "./gesture": {
      "import": "./dist/gesture/index.js",
      "require": "./dist/gesture/index.js",
      "types": "./dist/gesture/index.d.ts"
    },
    "./environmental": {
      "import": "./dist/environmental/index.js",
      "require": "./dist/environmental/index.js",
      "types": "./dist/environmental/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}