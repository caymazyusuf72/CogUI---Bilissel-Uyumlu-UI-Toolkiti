{
  "name": "@cogui/ml",
  "version": "0.1.0",
  "description": "CogUI AI/ML Integration - Behavioral prediction and cognitive analysis with TensorFlow.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogui/cogui.git",
    "directory": "packages/ml"
  },
  "keywords": [
    "cogui",
    "ai",
    "machine-learning",
    "tensorflow",
    "cognitive-analysis",
    "behavior-prediction",
    "accessibility",
    "neuroplasticity",
    "adaptive-ui"
  ],
  "author": "CogUI Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "@tensorflow/tfjs-data": "^4.10.0",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.1.6",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0"
  },
  "peerDependencies": {
    "@cogui/core": "workspace:*",
    "@cogui/sensors": "workspace:*",
    "react": ">=16.8.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.js", 
      "types": "./dist/models/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}